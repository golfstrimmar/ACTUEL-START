section(id='realise').realise.rel

  .container
    .title
      h3 Форма выпуска
    ul.realise__body
      li.realise__item.realise-item
        .realise-item__img.rel
          .imgs
            img(src='./img/i-4.webp' alt='img')
          svg
            use(xlink:href='#like')

        .realise-item__col
          .realise-item__title 
            h4 Nuxe Сплендьез Крем SPF 20 50мл
          .realise-item__fidback 
            fildset.fildset-rating
              .fildset-rating__items
                input(type="radio" id="fidback5" name='fidback' value='5' )
                label(for="fidback5")
                  svg
                    use(xlink:href='#star-empty')  
                input(type="radio" id="fidback4" name='fidback' value='4' )
                label(for="fidback4")
                  svg
                    use(xlink:href='#star-empty') 
                input(type="radio" id="fidback3" name='fidback' value='3' )
