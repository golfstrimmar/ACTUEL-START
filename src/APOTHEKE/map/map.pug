            button(type='button')  Балашиха
            button(type='button')  Бронницы
            button(type='button')  Высоковск
            button(type='button')  Все аптеки

          .accord__hidden._accord-hidden-js
            .accord-hidden__wrap._accord-hidden-wrap-js
              .map__picture.rel
                .imgs
                  img(src='./img/i-14.webp' alt='img')

              .map__daten
                include moc.pug
