section#bunner-slider.bunner-slider
  .swiper-pagination
  .slider-bunner.slider-bunner-js.swiper-container
    ul.bunner-slider__body.swiper-wrapper
      -
        var bunerFaq = [
          {
            src: './img/paralax.webp',
            title: "Title slide 1. ",
            title1: "Molestias, provident.",
          },

          {
            src: './img/img-1.webp',
            title: "Title slide 2.  ",
            title1: 'Lorem ipsum dolor.'
          },
          {
            src: './img/img-2.webp',
            title: "Title slide 3.  ",
            title1: 'Lorem ipsum dolor.'
          },
        ];

      each entry in bunerFaq
        li.slider__item.slider-card.swiper-slide
          .slider-card__img.imgs
            img(data=entry.src alt='img')
          .container
            .slider-card__body
              .slider-card__title
                h1 Lorem ipsum dolor sit amet.
              p.slider-card__text Lorem ipsum .
              a.btn.btn--success.but-wave.popups-init-js(rel='#popup1' href='#!')
                p form
  .bunner-search
    form.form-input
      input#search1(type='text' placeholder='Leškoti...' name="" )
      label(for='search1') Leškoti...
      .form-input__submit
        input(type='submit')
        svg
          use(xlink:href='#search')
